@model PagedList.IPagedList<ReturnDetail>
@using PagedList.Mvc;
@using PagedList;
@{
    var PageUtils = new PageUtils();
    UserPage user = new UserPage();
}
@foreach (var tracking in Model)
{
    <tr class="item">
        <td class="center">
            @if (tracking.PackageReturn.StatusId != 16)
             {
                <label class="pos-rel">
                    <input id="@tracking.Id" data-parent="false" data-pre="@tracking.Id" data-id="@tracking.Id" type="checkbox" class="ace" />
                    <span class="lbl"></span>
                </label>
            }
        </td>
        <td>@tracking.TrackingDetail.StorageJP.TrackingCode</td>
        <td>TH</td>
        @*<td>@tracking.TrackingDetail.TrackingSubCode</td>*@
    </tr>
}
<tr>
    <td colspan="3">
        <div class="pagging storejp-pagging">
            @Html.PagedListPager(Model, page => Url.Action("PackageReturnDetail", "Ajax", new { page = page, id = ViewBag.Id }), PagedListRenderOptions.ClassicPlusFirstAndLast)
        </div>
    </td>
</tr>
